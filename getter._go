package main

import (
    "fmt"
    "net/http"
    "io/ioutil"
    "os"
    )

func main() {
    response, err := http.Get("http://api.tumblr.com/v2/blog/vintage-fashionista.tumblr.com/posts?api_key=7SWYhNO0IuuMAp7rpSGyDrd5r5wSBJko2CGZQrhK8ConEK1c66")
    if err != nil {
        fmt.Printf("%s", err)
        os.Exit(1)
    } else {
        defer response.Body.Close()
        contents, err := ioutil.ReadAll(response.Body)
        if err != nil {
            fmt.Printf("%s", err)
            os.Exit(1)
        }
        fmt.Printf("%s\n", string(contents))
    }
}
